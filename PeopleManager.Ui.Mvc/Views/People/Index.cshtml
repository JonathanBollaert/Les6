@model IList<Person>

@{
    ViewData["Title"] = "People List";
}

<a asp-action="Create" class="btn btn-primary">New Person</a>

<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Organization</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    
    @foreach (var person in Model)
    {
        <partial name="PersonRowPartial" model="person"/>
    }
    </tbody>
</table>